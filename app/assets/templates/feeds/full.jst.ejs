<div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        <h4 class="modal-title"><%= feed.get('title') %></h4>
      </div>
      <div class="modal-body">
        <%= feed.get('description') %>
        <% if (feed.get('subscribed')) { %>
          <div class="feed-modal-unsubscribe">
            <span class="glyphicon glyphicon-minus" aria-hidden="true" alt="unsubscribe"></span>
          </div>
        <% } else { %>
          <div class="feed-modal-subscribe">
            <span class="glyphicon glyphicon-plus" aria-hidden="true" alt="subscribe"></span>
          </div>
        <% } %>
        <div class="feed-posts">
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
