<!DOCTYPE html>
<html>
<head>
  <title>Nconnected</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <link rel="icon" href="/favicon.png" type="image/x-icon"> 
</head>
<body>
<% if flash[:errors] %>
  <% flash[:errors].each do |error| %>
    <li><%= error %></li>
  <% end %>
<% end %>
<%= yield %>

</body>
</html>
